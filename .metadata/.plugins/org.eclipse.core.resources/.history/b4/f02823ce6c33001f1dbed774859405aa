<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
    
    
    <%
    
    // 세션은 한번 저장해놓으면 만료되기전 브라우저 끄기 전 까지
    // 아무곳에서나 사용 가능
    //object 타입
    
   	String user_id = (String) session.getAttribute("user_id");
    
    
    %>
<!DOCTYPE html>
<html>
<head>
<meta charset="UTF-8">
<title>Insert title here</title>
</head>
<body>

 <%= user_id %>
 <%= user_name %>

</body>
</html>